<configurationlogback.xml>
  <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <Pattern>%d{HH:mm:ss.SSS} %-5level %-10([%thread]) %logger{0} - %m%n</Pattern>
    </encoder>
  </appender>



  <root level="DEBUG">
    <appender-ref ref="STDOUT"/>
  </root>
</configurationlogback.xml>
